{
    "compilerOptions": {
        "module": "CommonJS",
        "moduleResolution": "node",
        "target": "es6",
        "baseUrl": "./https://pdubbssolutions.ca",
        "jsx": "react",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "lib": ["ES2022", "DOM"],
        "resolveJsonModule": true
    },
    "exclude": [
        "node_modules"
    ],
    "include": [
        "src/**/*"
, "public/server.js", "public/index.mjs", "public/app.js", "public/express.js"    ]
}
